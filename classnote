 /* PART 2 +-=-= */


/* BOX MODEL CSS */
div {
    height: 100px;
    border: 2px;
    border-color: black;
    border-style: solid;
}
/*border SHORTHAND  */
div {
    border: 3px , solid,brown;
    border-radius: 20px;
    padding: 5px;padding: 2px,3px,4px,5px;
 }
 /* PADDING - Margin*/

 div {
    border: 3px , solid,brown;
    border-radius: 20px;
    padding: 5px;padding: 2px,3px,4px,5px;
    margin: 23px;
 }
/* DISPLAY ELEMENTS */
div {
    color : rosybrown ;
    height: 100px;
    width: 100px;
    margin: 25px;
    padding: 25px;
    display: inline-block;
    display: block;
    display: inline;
    display: none;
}

/* visibility */
box {
    visibility: hidden;
    /*  */
}
 /* alpha Channel*/
 div {
    
    background-color: rgba(183, 99, 103, 0.75);
}